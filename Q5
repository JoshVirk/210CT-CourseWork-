A = [[9,21,4]]#Matrices used for the calculation
B = [[12,6,15]]
Ans = [[0,0,0]]

print("The original matrices:")
print(A)#Shows the original list
print(B)
for i in range(len(A)):#iterate through rows
   for j in range(len(A[0])):#iterate through columns
       Ans[i][j] = A[i][j] + B[i][j]#Calculation of the two matrices

for An in Ans:
   print("The addition of the matrices:")
   print(An)#Prints answer with which calculation it was

for i in range(len(A)):
   for j in range(len(A[0])):
       Ans[i][j] = A[i][j] * B[i][j]#Calculation of the two matrices

for An1 in  Ans:
    print ("The multiplication of the matrices:")
    print (An1)#Prints answer with which calculation it was

for i in range(len(A)):
   for j in range(len(A[0])):
       Ans[i][j] = A[i][j] / B[i][j]#Calculation of the two matrices

for An2 in  Ans:
    print ("The Division of the matrices:")
    print (An2)#Prints answer with which calculation it was

for i in range(len(A)):
   for j in range(len(A[0])):
       Ans[i][j] = A[i][j] * B[i][j]-2*(A[i][j]+B[i][j])#Calculation of the two matrices
